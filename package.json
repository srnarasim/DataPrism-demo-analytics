{
  "name": "dataprism-demo-analytics",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Standalone DataPrism analytics demo application - consumes DataPrism via CDN",
  "keywords": [
    "dataprism",
    "analytics",
    "demo",
    "cdn",
    "standalone"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "test": "vitest run tests/unit",
    "test:unit": "vitest run tests/unit",
    "test:unit:watch": "vitest tests/unit",
    "test:unit:coverage": "vitest run tests/unit --coverage",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "vitest run tests/performance",
    "test:security": "vitest run tests/security",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance && npm run test:security && npm run test:e2e",
    "validate": "npm run test:all && npm run lint && npm run type-check",
    "validate:prp": "npm run test:unit",
    "validate:ci": "npm run test:unit && npm run type-check",
    "audit:lighthouse": "lighthouse http://localhost:3000 --config-path=lighthouse.config.js",
    "test:cdn": "vitest run tests/unit/cdnValidator.test.ts",
    "check:bundle-size": "npm run build && bundlesize",
    "monitor:performance": "vitest run tests/performance/lighthouse.test.ts",
    "status:cdn": "curl -I https://srnarasim.github.io/DataPrism/manifest.json",
    "report:validation": "npm run test:all -- --reporter=json > test-results/validation-report.json",
    "health:check": "npm run status:cdn && npm run test:unit:coverage",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@observablehq/plot": "^0.6.11",
    "@tanstack/react-query": "^4.35.0",
    "@tanstack/react-table": "^8.10.0",
    "chart.js": "^4.4.0",
    "clsx": "^2.0.0",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.263.1",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.15.0",
    "recharts": "^2.8.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/d3": "^7.4.0",
    "@types/papaparse": "^5.3.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.15",
    "chrome-launcher": "^1.2.0",
    "eslint": "^8.50.0",
    "gh-pages": "^6.0.0",
    "jest-environment-jsdom": "^30.0.4",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.0",
    "puppeteer": "^24.12.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
